<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<TargetFramework>net472</TargetFramework>
    <AssemblyName>StayInTarkov</AssemblyName>
    <Description>Stay in Tarkov plugin for Escape From Tarkov</Description>
    <Version>1.0.0</Version>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <LangVersion>preview</LangVersion>
	<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
	<Deterministic>false</Deterministic>
	<Copyright>Copyright @ Stay In Tarkov 2024</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Coop\Matchmaker\MatchmakerAccept\Grouping\**" />
    <Compile Remove="Networking\Packets\**" />
    <EmbeddedResource Remove="Coop\Matchmaker\MatchmakerAccept\Grouping\**" />
    <EmbeddedResource Remove="Networking\Packets\**" />
    <None Remove="Coop\Matchmaker\MatchmakerAccept\Grouping\**" />
    <None Remove="Networking\Packets\**" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources\Language\English.json">
    </EmbeddedResource>
    <EmbeddedResource Include="Resources\Language\French.json" />
    <EmbeddedResource Include="Resources\Language\Japanese.json">
    </EmbeddedResource>
    <EmbeddedResource Include="Resources\Language\SimplifiedChinese.json">
    </EmbeddedResource>
    <EmbeddedResource Include="Resources\Language\TraditionalChinese.json">
    </EmbeddedResource>
	<EmbeddedResource Include="Resources\Language\German.json">
	</EmbeddedResource>
  </ItemGroup>

